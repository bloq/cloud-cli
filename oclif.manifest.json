    "clusters": {
      "id": "clusters",
      "description": "Manage your BloqCloud clusters",
      "pluginName": "@bloq/cloud-cli",
      "pluginType": "core",
      "aliases": [],
      "flags": {
        "serviceId": {
          "name": "serviceId",
          "type": "option",
          "char": "s",
          "description": "service id"
        },
        "authType": {
          "name": "authType",
          "type": "option",
          "char": "t",
          "description": "auth type (jwt or basic)",
          "helpValue": "(jwt|basic)",
          "options": ["jwt", "basic"],
          "default": "basic"
        },
        "capacity": {
          "name": "capacity",
          "type": "option",
          "char": "c",
          "description": "capacity",
          "default": 2
        },
        "all": {
          "name": "all",
          "type": "boolean",
          "char": "a",
          "description": "list all clusters",
          "required": false,
          "allowNo": false
        },
        "clusterId": {
          "name": "clusterId",
          "type": "option",
          "char": "i",
          "description": "cluster id"
        }
      },
      "args": [
        {
          "name": "operation",
          "description": "Specify the type of cluster operation to run",
          "required": true,
          "options": ["create", "list", "remove", "info", "chains", "services"],
          "default": "list"
        }
      ]
    },
    "connect": {
      "id": "connect",
      "description": "Access Connect services through bcl",
      "pluginName": "@bloq/cloud-cli",
      "pluginType": "core",
      "aliases": [],
      "flags": {
        "argument": {
          "name": "argument",
          "type": "option",
          "char": "a",
          "description": "Specify the argument for the method"
        },
        "chain": {
          "name": "chain",
          "type": "option",
          "char": "c",
          "description": "Specify the chain for the method",
          "options": ["btc", "bch"],
          "default": "btc"
        },
        "network": {
          "name": "network",
          "type": "option",
          "char": "n",
          "description": "Specify the network for the method",
          "options": ["mainnet"],
          "default": "mainnet"
        }
      },
      "args": [
        {
          "name": "method",
          "description": "Specify the method to get from connect API",
          "required": true,
          "options": [
            "block",
            "blocks",
            "block-hash",
            "raw-block",
            "transaction",
            "tx",
            "raw-transaction",
            "raw-tx"
          ]
        }
      ]
    },
    "events": {
