{
  "version": "2.3.0",
  "commands": {
    "client-keys": {
      "id": "client-keys",
      "description": "Manage your BloqCloud client key(s)",
      "pluginName": "@bloq/cloud-cli",
      "pluginType": "core",
      "aliases": [],
      "flags": {
        "clientId": {
          "name": "clientId",
          "type": "option",
          "char": "i",
          "description": "client id"
        }
      },
      "args": [
        {
          "name": "operation",
          "description": "Specify the type of client-keys operation to run",
          "required": true,
          "options": ["create", "list", "remove"],
          "default": "list"
        }
      ]
    },
    "client-token": {
      "id": "client-token",
